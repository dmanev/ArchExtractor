     54          // EEPROMErrorCounter is divided is 3 parts :
     55          //                     7  6  5  4  3  2  1  0
     56          //                     X  X  X  X  X  X  X  X
     57          //       Rebuild flag _|  |  |  |  |  |  |  |_
     58          //                   _____|  |  |  |  |  |____| 
     59          // ReadErrorCounter_|________|  |  |  |_______|-WriteErrorCouter
     60          //                  |___________|  |__________| 
     61          #define cMaxReadCounter   ((U8) 0x07)
