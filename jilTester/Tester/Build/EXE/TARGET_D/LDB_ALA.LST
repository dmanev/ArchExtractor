    131              mLDBRegWrite1BitInRegister(ASIM6, 7, 1); \
    132              mLDBRegWrite1BitInRegister(ASIM6, 6, 1); \
    133              mLDBRegWrite1BitInRegister(ASIM6, 5, 0); \
    134              /* port must be '1' when UART enabled: */ \
    135              mLDBHarWrite(cLDBHarAlarmOut,1); \
